<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<dataset>
  <registry-descriptor>
    <display-name>Gear Poll Vote</display-name>
    <description>Store information when a user submits a response to a Poll gear in a portal application</description>
    <timekey>true</timekey>
    <mapper>/PollVoteSqlLogger.xml</mapper>
    <id>/gearpollvote.xml</id>
  </registry-descriptor>
  <sampling>
    <filter>all</filter>
    <units>percent</units>
    <rate>1</rate>
  </sampling>
</dataset>
