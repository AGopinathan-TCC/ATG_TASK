<?xml version="1.0" encoding="utf-8"?>

<dynamo-message-system>

  <!-- message registry definitions -->
  <message-registry>
    <message-family>
      <message-family-name>
        paf
      </message-family-name>

      <message-type>
        <jms-type>
          atg.portal.gear.calendar.EventCreatedMessage
        </jms-type>
	      <message-class>
	        atg.portal.gear.calendar.EventCreatedMessage
	      </message-class>
	      <message-context>
	        session
	      </message-context>
	      <display-name-resource>
	        eventCreatedMessageDisplayName
	      </display-name-resource>
	      <description-resource>
	        eventCreatedMessageDescription
	      </description-resource>
	      <resource-bundle>
	        atg.portal.gear.calendar.CalendarMessagingSystemResources
	      </resource-bundle>
      </message-type>

      <message-type>
        <jms-type>
	        atg.portal.gear.calendar.EventDeletedMessage
        </jms-type>
	      <message-class>
	        atg.portal.gear.calendar.EventDeletedMessage
	      </message-class>
	      <message-context>
	        session
	      </message-context>
	      <display-name-resource>
	        eventDeletedMessageDisplayName
	      </display-name-resource>
	      <description-resource>
	        eventDeletedMessageDescription
	      </description-resource>
	      <resource-bundle>
	        atg.portal.gear.calendar.CalendarMessagingSystemResources
	      </resource-bundle>
      </message-type>

      <message-type>
        <jms-type>
          atg.portal.gear.calendar.EventEditedMessage
        </jms-type>
	      <message-class>
	        atg.portal.gear.calendar.EventEditedMessage
	      </message-class>
	      <message-context>
	        session
	      </message-context>
	      <display-name-resource>
	        eventEditedMessageDisplayName
	      </display-name-resource>
	      <description-resource>
	        eventEditedMessageDescription
	      </description-resource>
	      <resource-bundle>
	        atg.portal.gear.calendar.CalendarMessagingSystemResources
	      </resource-bundle>
      </message-type>

      <message-type>
        <jms-type>
	        atg.portal.gear.calendar.EventViewedMessage
        </jms-type>
	      <message-class>
	        atg.portal.gear.calendar.EventViewedMessage
	      </message-class>
	      <message-context>
	        session
	      </message-context>
	      <display-name-resource>
	        eventViewedMessageDisplayName
	      </display-name-resource>
	      <description-resource>
	        eventViewedMessageDescription
	      </description-resource>
	      <resource-bundle>
	        atg.portal.gear.calendar.CalendarMessagingSystemResources
	      </resource-bundle>
      </message-type>

    </message-family>
  </message-registry>
  
</dynamo-message-system>
