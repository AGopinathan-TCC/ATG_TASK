<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE portal-manifest PUBLIC "-//Art Technology Group, Inc.//DTD Portal Manifest//EN" "http://www.atg.com/dtds/portal/framework/portal_manifest_1.0.dtd">
<portal-manifest version="1.0" author="ATG Portlet Deploy Tool" name="WSRP4ATGProxyPortlet" >
  <gear-definitions>
    <gear-definition name="WSRP4ATGProxyPortlet" version="1.0" author="ATG Portlet Deploy Tool">
      <description>A generic proxy-Portlet for WSRP integration with ATG Portal.</description>
      <servlet-context>/portlet/WSRPProxyPortlet</servlet-context>
      <dimensions>
	<dimension name="height">noPreference</dimension>
	<dimension name="width">narrow</dimension>
      </dimensions>
      <gear-modes>
	<gear-mode name="content">
	  <display-modes>
	    <display-mode name="shared">
	      <device-output name="html">/WSRPProxyPortlet</device-output> 
	    </display-mode>
	    <display-mode name="full">
	      <device-output name="html">/WSRPProxyPortlet</device-output>
	    </display-mode>
	  </display-modes>
	</gear-mode>
      </gear-modes>
      <jps-portlet value="true">
	<portlet-guid value="ProxyPortlet"></portlet-guid>
      </jps-portlet>
      <instance-parameters>
	<parameter name="atg.portlet-class">
	  <default-value>atg.wsrp.consumer.ProxyPortlet</default-value>
	</parameter>  
      </instance-parameters>
    </gear-definition>
  </gear-definitions>
</portal-manifest>
