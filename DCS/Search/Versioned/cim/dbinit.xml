<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE database-initializer SYSTEM "http://www.atg.com/dtds/dbinit_1_0.dtd">
<database-initializer>

  <schema id="versionedSchema">
    <sql>
      <path>
        <create>
          sql/db_components/${database}/versioned_commerce_refinement_ddl.sql
        </create>
        <drop>
          sql/uninstall/${database}/drop_versioned_commerce_refinement_ddl.sql
        </drop>
      </path>
    </sql>
    <sql>
      <path>
        <create>
          sql/db_components/${database}/versioned_custom_catalog_refinement_ddl.sql
        </create>
        <drop>
          sql/uninstall/${database}/drop_versioned_custom_catalog_refinement_ddl.sql
        </drop>
      </path>
    </sql>
  </schema>

  <server-instance-type id="management">
    <datasource id="all">
      <schema id="versionedSchema"/>
    </datasource>  
  </server-instance-type>
  
</database-initializer>
