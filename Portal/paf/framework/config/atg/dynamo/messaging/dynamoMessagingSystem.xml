	      </description-resource>
	      <resource-bundle>
	        atg.portal.messaging.GearMessagingSystemResources
	      </resource-bundle>
      </message-type>
      
      <message-type>
        <jms-type>
	        atg.portal.messaging.PageGearAddedMessage
        </jms-type>
	      <message-class>
	        atg.portal.messaging.PageGearAddedMessage
	      </message-class>
	      <message-context>
	        session
	      </message-context>
	      <display-name-resource>
	        PageGearAddedMessageDisplayName
