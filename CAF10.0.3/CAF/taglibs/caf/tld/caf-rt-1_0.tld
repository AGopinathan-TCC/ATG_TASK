    <body-content>JSP</body-content>
    <description>Column-based layout container, allowing objects to be grouped in columns on the page</description>

    <attribute>
      <name>id</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The id used to identify the layout column object</description>
