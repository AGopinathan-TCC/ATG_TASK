    <attribute>
      <name>width</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The width of the layout row in pixels or percentages</description>
    </attribute>

    <attribute>
      <name>height</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The height of the layout row in pixels or percentages</description>
    </attribute>

    <attribute>
      <name>packRow</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>A boolean value (default is true) that will automatically pack the row to ensure that specified object sizes are rendered correctly</description>
    </attribute>
  </tag>

  <!-- ============================== Validation Tags ============================== -->

  <!-- =========================================== -->
  <!-- validation                                  -->
  <!-- =========================================== -->
  <tag>
    <name>validation</name>
    <tag-class>atg.web.taglib.ui.validation.Validation</tag-class>
    <body-content>JSP</body-content>
    <description>The ValidationMap tag allows the various input objects and validation types to be tied together to create a validation instance on a page</description>

    <attribute>
      <name>validationFunction</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The name of the JavaScript function that will be called by the form handler prior to submission</description>
    </attribute>

    <attribute>
      <name>initializeFunction</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The name of the Javascript initialization function that is called to initialize every validation</description>
