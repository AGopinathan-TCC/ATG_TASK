    <attribute>
      <name>width</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The width of the layout column in pixels or percentages</description>
    </attribute>

    <attribute>
      <name>height</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The height of the layout column in pixels or percentages</description>
    </attribute>

    <attribute>
      <name>packColumn</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
