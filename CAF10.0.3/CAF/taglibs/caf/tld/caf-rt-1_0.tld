<?xml version="1.0"?>

<!-- ======================================================================= -->
<!-- Tag Library Descriptor for the CAF tag library.                         -->
<!-- ======================================================================= -->

<!DOCTYPE taglib
  PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
  "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>
  <tlib-version>1.0</tlib-version>
  <jsp-version>1.2</jsp-version>
  <short-name>caf</short-name>
  <uri>http://www.atg.com/taglibs/caf</uri>
  <display-name>Common Application Framework Tags</display-name>
  <description>Common Application Framework 1.0 tag library</description>


  <!-- =========================================== -->
  <!-- getPropertyValue                            -->
  <!-- =========================================== -->
  <tag>
    <name>getPropertyValue</name>
    <tag-class>atg.caf.taglib.GetPropertyValueTag</tag-class>
    <tei-class>atg.caf.taglib.GetPropertyValueTag$TEI</tei-class>
    <body-content>JSP</body-content>
    <description>Gets a named property of a java bean</description>

    <attribute>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Sets the name of the scripting variable that will hold the results</description>
    </attribute>

    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Sets the name of the attribute that will hold the results</description>
    </attribute>

    <attribute>
        <name>scope</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
      <description>Sets the scope that the var should be set in.  Valid values are [page|request|session|application]</description>
    </attribute>

    <attribute>
      <name>target</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Target object whose property will be retrieved</description>
    </attribute>

    <attribute>
      <name>property</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Sets the name of the property of the 'target' object to access</description>
    </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- getKeySet                                   -->
  <!-- =========================================== -->
  <tag>
    <name>getKeySet</name>
    <tag-class>atg.caf.taglib.GetKeySetTag</tag-class>
    <tei-class>atg.caf.taglib.GetKeySetTag$TEI</tei-class>
    <body-content>JSP</body-content>
    <description>Gets the key set from a given map</description>

    <attribute>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Sets the name of the scripting variable that will hold the results</description>
    </attribute>

    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Sets the name of the attribute that will hold the results</description>
    </attribute>

    <attribute>
        <name>map</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
      <description>Sets the map to get the key set from</description>
    </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- size                                        -->
  <!-- =========================================== -->
  <tag>
    <name>size</name>
    <tag-class>atg.caf.taglib.SizeTag</tag-class>
    <tei-class>atg.caf.taglib.SizeTag$TEI</tei-class>
    <body-content>JSP</body-content>
    <description>
	Tag that calculates the size of a given Collection or the length of a given String object.
	Values are specified by the 'collection' or 'string' attributes respecitvely.  The tag
	then stores the size or length in a page variable, specified by the 'var' attribute.
	If a value for 'collection' and 'string' is specified, the size of the collection
	will be returned.
    </description>

    <attribute>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Sets the name of the scripting variable that will hold the results</description>
    </attribute>

    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Sets the name of the attribute that will hold the results</description>
    </attribute>

    <attribute>
      <name>collection</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Specifies the collection of items</description>
    </attribute>

    <attribute>
      <name>string</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Specifies the string to get the length of</description>
    </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- tokensToList                                -->
  <!-- =========================================== -->
  <tag>
    <name>tokensToList</name>
    <tag-class>atg.caf.taglib.TokensToListTag</tag-class>
    <tei-class>atg.caf.taglib.TokensToListTag$TEI</tei-class>
    <body-content>JSP</body-content>
    <description>
      A tag to convert a delimited string of tokens into a list
      containing the tokens.  Works similarly to the JSTL forTokens
      tag, except that it returns a list of tokens rather than iterating
      over the token set.
    </description>

    <attribute>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Sets the name of the scripting variable that will hold the results</description>
    </attribute>

    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Sets the name of the attribute that will hold the results</description>
    </attribute>

    <attribute>
        <name>scope</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
      <description>Sets the scope that the var should be set in.  Valid values are [page|request|session|application]</description>
    </attribute>

    <attribute>
      <name>items</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>A string delimited by tokens.</description>
    </attribute>

    <attribute>
      <name>delims</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The delimiter string to delimit the string passed into the 'items' attribute.</description>
    </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- search                                      -->
  <!-- =========================================== -->
  <tag>
    <name>search</name>
    <tag-class>atg.caf.taglib.SearchTag</tag-class>
    <tei-class>atg.caf.taglib.SearchTag$TEI</tei-class>
    <body-content>JSP</body-content>
    <description>Performs a search using a SearchService and a SearchContext</description>

    <attribute>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Sets the name of the scripting variable that will hold the search results</description>
    </attribute>

    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Sets the name of the attribute in the page context that will hold the search results</description>
    </attribute>

    <attribute>
      <name>searchService</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Sets the SearchService to use when performing searches</description>
    </attribute>

    <attribute>
      <name>searchContext</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Sets the SearchContext used to describe the type of search desired.</description>
    </attribute>

    <attribute>
      <name>findSimilar</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>A flag indicating to whether to do a regular search or a search for similar results</description>
    </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- sort                                        -->
  <!-- =========================================== -->
  <tag>
    <name>sort</name>
    <tag-class>atg.caf.taglib.SortTag</tag-class>
    <tei-class>atg.caf.taglib.SortTag$TEI</tei-class>
    <body-content>JSP</body-content>
    <description>Takes a collection of beans and a bean property name and returns the collection as a List sorted in ascending order by the named bean property.</description>

    <attribute>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Sets the name of the scripting variable that will hold the results</description>
    </attribute>

    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Sets the name of the attribute that will hold the results</description>
    </attribute>

    <attribute>
      <name>collection</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Specifies the collection of items</description>
    </attribute>

    <attribute>
      <name>property</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Specifies the property to sort the collection by</description>
    </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- checkAccessController                       -->
  <!-- =========================================== -->
  <tag>
    <name>checkAccessController</name>
    <tag-class>atg.caf.taglib.CheckAccessControllerTag</tag-class>
    <tei-class>atg.caf.taglib.CheckAccessControllerTag$TEI</tei-class>
    <body-content>JSP</body-content>
    <description>Returns a boolean variable indicating if a given AccessController accepts a given Profile.</description>

    <attribute>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Sets the name of the scripting variable that will hold the results</description>
    </attribute>

    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Sets the name of the variable that will hold the results</description>
    </attribute>

    <attribute>
      <name>accessController</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Specifies the instance of atg.userprofiling.AccessController</description>
    </attribute>

    <attribute>
      <name>profile</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Specifies the instance of atg.userprofiling.Profile</description>
    </attribute>
  </tag>

  <!-- ============================== Calendar Tags ============================== -->

  <!-- =========================================== -->
  <!-- calendarInstance                            -->
  <!-- =========================================== -->
  <tag>
      <name>calendarInstance</name>
      <tag-class>atg.web.taglib.ui.calendar.CalendarInstance</tag-class>
      <body-content>JSP</body-content>
      <description>Creates a JavaScript object that is used to represent a single instance of a calendar on the client</description>

      <attribute>
        <name>calendarInstanceId</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
        <description>The id used to identify the calendar instance</description>
      </attribute>

      <attribute>
        <name>containerId</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
        <description>The id for the container in which the calendar is displayed</description>
      </attribute>

      <attribute>
        <name>anchorId</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
        <description>The id for the anchor object that is used for position of the calendar</description>
      </attribute>

      <attribute>
        <name>dateInputId</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>The id of the date input object that is used to communicate the date to and from the calendar</description>
      </attribute>

      <attribute>
        <name>curtainId</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>The id of the date input object that is used to communicate the date to and from the calendar</description>
      </attribute>

      <attribute>
        <name>stylesId</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
        <description>The id for the styles JavaScript object, used to provide static style mappings to the calendar</description>
      </attribute>

      <attribute>
        <name>i18nId</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
        <description>The id for the i18n JavaScript object, used to provide static internationalization information to the calendar</description>
      </attribute>

      <attribute>
        <name>openDate</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>The open date String that can be used if a date input object is not specified</description>
      </attribute>

      <attribute>
        <name>horizontalOffset</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>The horizontal offset in pixels from the anchor object</description>
      </attribute>

      <attribute>
        <name>verticalOffset</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>The vertical offset in pixels from the anchor object</description>
      </attribute>

      <attribute>
        <name>closeAfterSelection</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>A boolean that automatically closes the calendar when a date is selected</description>
      </attribute>

      <attribute>
        <name>hasToday</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>A boolean that displays the today link underneath the current month</description>
      </attribute>

      <attribute>
        <name>hasClose</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>A boolean that displays the close link underneath the current month</description>
      </attribute>

      <attribute>
        <name>displayBelowAnchor</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>A boolean that determines whether the calendar is displayed below (default) or above the anchor object</description>
      </attribute>

      <attribute>
        <name>displayRightOfAnchor</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>A boolean that determines whether the calendar is displayed to the right (default) or to the left of the anchor object</description>
      </attribute>

      <attribute>
        <name>autoFlip</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>A boolean that will automatically flip the calendar if there is not sufficient space to display it in the requested quadrant</description>
      </attribute>

      <attribute>
        <name>clearDateOnError</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>A boolean that will clear the entire date input field when a validation error occurs. By default, this value is set to true</description>
      </attribute>

      <attribute>
        <name>setDateOnCalendarOpen</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>A boolean that will automatically set the opening date of the calendar into the input text box when the calendar is opened</description>
      </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- calendarI18n                                -->
