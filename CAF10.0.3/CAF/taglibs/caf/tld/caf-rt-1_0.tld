      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>

    <attribute>
      <name>iconOpened</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>

    <attribute>
      <name>iconClosed</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>

    <attribute>
      <name>iconCheck</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>

    <attribute>
      <name>checkboxes</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>

    <attribute>
      <name>canDrag</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>

    <attribute>
       <name>dragIcon</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
     </attribute>

     <attribute>
       <name>canDrop</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
     </attribute>

      <attribute>
        <name>dropTypes</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
      </attribute>

     <attribute>
       <name>initStateOpen</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
       <description>An optional attribute that will trigger the opening of the branch after created.</description>
      </attribute>

     <attribute>
      <name>initStateChecked</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>An optional attribute that will have the check box pre-checked.</description>
     </attribute>

     <attribute>
      <name>initStateSelected</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>An optional attribute that will have the branch pre-selected.</description>
     </attribute>

    <attribute>
      <name>loadActionParams</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>name value pairs ( semi-colon seperated). "name=value;name2=value2". This are appended to the loadAction url</description>
    </attribute>

    <attribute>
      <name>dynamicProperties</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>name value pairs ( semi-colon seperated). "name=value;name2=value2". This will become properties of the created branch object</description>
    </attribute>

  </tag>

  <!-- =========================================== -->
  <!-- Action                                      -->
  <!-- =========================================== -->
  <tag>
    <name>action</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.Action</tag-class>
    <display-name>Partial Page Renderer Action Component Tag</display-name>
    <description>Tag that identifies a server action to invoke for reloading data. The data should be returned in an XML document for processing by output elements.</description>

    <attribute>
      <name>actionId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Unique identifier for a partial page renderer action.</description>
    </attribute>

    <attribute>
      <name>bean</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The form handler bean.</description>
    </attribute>

    <attribute>
      <name>debugElementId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The valid DOM id of a page element to which to direct the debugging output.</description>
    </attribute>

    <attribute>
      <name>debugLevel</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The debug level either "none" (default) or "verbose".</description>
    </attribute>

    <attribute>
      <name>errorUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The form handler error URL.</description>
    </attribute>

    <attribute>
      <name>handler</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The form handler handle method.</description>
    </attribute>

    <attribute>
      <name>method</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The form handler HTTP method.</description>
    </attribute>

    <attribute>
      <name>protocol</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The server action format either "struts" or "formhandlers".</description>
    </attribute>

    <attribute>
      <name>recordHistory</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Indicates whether the action inserts each transaction in the browser history.</description>
    </attribute>

    <attribute>
      <name>redirectToErrorUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>If true, the partial page renderer redirects to the error URL when an error occurs. Otherwise the partial page renderer parses the contents of the error URL without a full page reload.</description>
    </attribute>

    <attribute>
      <name>successUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The form handler success URL.</description>
    </attribute>

    <attribute>
      <name>synchronizeTransaction</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>If true, transaction is synchronized. The default is asynchronous transactions.</description>
    </attribute>

    <attribute>
      <name>url</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Context-relative URL to a server action. The server action should forward to data in an XML format for rendering via XSLT in output elements.</description>
    </attribute>

    <attribute>
      <name>useRequestDispatcherForward</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>If true, the forward method on the request dispatcher is used to navigate to the success URL. Otherwise, an HTTP redirect is used, returning HTTP status 302 to the client and forcing the client to make a second request.</description>
    </attribute>

  </tag>

  <!-- =========================================== -->
  <!-- Input Element                               -->
  <!-- =========================================== -->
  <tag>
    <name>inputElement</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.InputElement</tag-class>
    <body-content>empty</body-content>
    <display-name>Partial Page Renderer Input Element Component Tag</display-name>
    <description>Tag that identifies an input element on the page for extracting data to post to a partial page renderer action.</description>

    <attribute>
      <name>accessScript</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>JavaScript evaluated by the partial page renderer to access the value of the input element for reading.</description>
    </attribute>

    <attribute>
      <name>isHistoryTitle</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>When history is enabled, indicates whether the value of the current input element contains the title for the history event.</description>
    </attribute>

    <attribute>
      <name>isQuery</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Indicates whether the name value pair is assigned to the query string of the partial page renderer action URL.</description>
    </attribute>

    <attribute>
      <name>sourceId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Unique identifier for a partial page renderer input element. This attribute must correspond to a valid HTML element ID supplied by the page author.</description>
    </attribute>

    <attribute>
      <name>property</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Name of the HTTP header under which the value of the input element is posted to the server or the property name corresponding to a valid servlet-configured form-bean property.</description>
    </attribute>

    <attribute>
      <name>value</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Constant value element.</description>
    </attribute>

  </tag>

  <!-- =========================================== -->
  <!-- Error Element                               -->
  <!-- =========================================== -->
  <tag>
    <name>errorElement</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.ErrorElement</tag-class>
    <body-content>empty</body-content>
    <display-name>Partial Page Renderer Error Element Component Tag</display-name>
    <description>Tag that identifies an element on the page to show errors following a partial page renderer action.</description>

    <attribute>
      <name>elementId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Unique identifier for an element to show the errors following a partial page renderer action. This attribute must correspond to a valid HTML element ID supplied by the page author.</description>
    </attribute>

  </tag>

  <!-- =========================================== -->
  <!-- Hide Element                               -->
  <!-- =========================================== -->
  <tag>
    <name>hideElement</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.HideElement</tag-class>
    <body-content>empty</body-content>
    <display-name>Partial Page Renderer Hide Element Component Tag</display-name>
    <description>Tag that identifies an element on the page to hide during a partial page renderer action.</description>

    <attribute>
      <name>elementId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Unique identifier for an element to hide during the partial page renderer action. This attribute must correspond to a valid HTML element ID supplied by the page author.</description>
    </attribute>

  </tag>

  <!-- =========================================== -->
  <!-- Show Element                               -->
  <!-- =========================================== -->
  <tag>
    <name>showElement</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.ShowElement</tag-class>
    <body-content>empty</body-content>
    <display-name>Partial Page Renderer Show Element Component Tag</display-name>
    <description>Tag that identifies an element on the page to show during a partial page renderer action, such as a please wait icon.</description>

    <attribute>
      <name>elementId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Unique identifier for an element to show during the partial page renderer action. This attribute must correspond to a valid HTML element ID supplied by the page author.</description>
    </attribute>

  </tag>

  <!-- =========================================== -->
  <!-- Output JavaScript                           -->
  <!-- =========================================== -->
  <tag>
    <name>outputJavaScript</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.OutputJavaScript</tag-class>
    <body-content>JSP</body-content>
    <display-name>Partial Page Renderer Output JavaScript Tag</display-name>
    <description>Tag that identifies JavaScript on the page for evaluation following a partial page renderer request.</description>
  </tag>

  <!-- =========================================== -->
  <!-- Output Status                               -->
  <!-- =========================================== -->
  <tag>
    <name>outputStatus</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.OutputStatus</tag-class>
    <body-content>JSP</body-content>
    <display-name>Partial Page Renderer Output Status Tag</display-name>
    <description>Tag that notifies the client about a condition on the server related to the request, such as an invalid session.</description>

    <attribute>
      <name>redirectUrl</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Optional URL to which to redirect when the given status condition occurs.</description>
    </attribute>
    <attribute>
      <name>statusKey</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Status key that identifies a condition on the server for the request. Possible values: "sessioninvalid".</description>
    </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- Output XHTML                                -->
  <!-- =========================================== -->
  <tag>
    <name>outputXhtml</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.OutputXhtml</tag-class>
    <body-content>JSP</body-content>
    <display-name>Partial Page Renderer Output XHTML Component Tag</display-name>
    <description>Tag that encloses output XHTML on the page for insertion into an element following a partial page renderer request.</description>

    <attribute>
      <name>append</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Indicates whether the output should be appended to the existing content of the specified output element, either "true" or "false". Default is false indicating that the contents of the target element are overwritten.</description>
    </attribute>
    <attribute>
      <name>targetId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Unique identifier for a partial page renderer output element. This attribute must correspond to a valid HTML element ID supplied by the page author.</description>
    </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- Output XML                                  -->
  <!-- =========================================== -->
  <tag>
    <name>outputXml</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.OutputXml</tag-class>
    <body-content>empty</body-content>
    <display-name>Partial Page Renderer Output XML Component Tag</display-name>
    <description>Tag that identifies an XSL stylesheet for application to the returned XML following a partial page renderer request.</description>

    <attribute>
      <name>accessScript</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>JavaScript evaluated by the partial page renderer to access the value of the output element for writing.</description>
    </attribute>

    <attribute>
      <name>targetId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Unique identifier for a partial page renderer target element. This attribute must correspond to a valid HTML element ID supplied by the page author.</description>
    </attribute>

    <attribute>
      <name>xsltString</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>XSLT stylesheet string. The XSLT stylesheet is applied to the results of the partial page renderer action, which should consist of a valid XML document.</description>
    </attribute>

    <attribute>
      <name>xsltUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Context-relative path to an XSLT stylesheet resource. The XSLT stylesheet is applied to the results of the partial page renderer action, which should consist of a valid XML document.</description>
    </attribute>

  </tag>

  <!-- =========================================== -->
  <!-- Transact                                    -->
  <!-- =========================================== -->
  <tag>
    <name>transact</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.Transact</tag-class>
    <body-content>empty</body-content>
    <display-name>Partial Page Renderer Invocation Component Tag</display-name>
    <description>Tag that causes the specified partial page renderer action to reload.</description>

    <attribute>
      <name>actionId</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Identifier of a partial page renderer action to invoke.</description>
    </attribute>

    <attribute>
      <name>synchronize</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Indicates whether the XMLHTTPRequest transaction is synchronized. Default value is false (asynchronous).</description>
    </attribute>
  </tag>

  <!-- =========================================== -->
  <!-- History                                     -->
  <!-- =========================================== -->
  <tag>
    <name>history</name>
    <tag-class>atg.web.taglib.ui.partialpagerenderer.History</tag-class>
    <body-content>empty</body-content>
    <display-name>Partial Page Renderer History Component Tag</display-name>
    <description>Tag that inserts partial page renderer action history into the browser history.</description>
  </tag>

</taglib>
