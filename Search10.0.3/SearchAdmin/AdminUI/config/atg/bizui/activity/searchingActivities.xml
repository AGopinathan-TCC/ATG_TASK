<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE generic-activities
        PUBLIC "-//Art Technology Group, Inc.//DTD ActivityManager configuration//EN"
        "http://www.atg.com/dtds/activitymanager/activitymanager_1.0.dtd">

<generic-activities>

  <activity>
    <id>searchBrowse</id>
    <resource-bundle>atg.searchadmin.adminui.activity.activityresources</resource-bundle>
    <display-name-resource>searchBrowseDisplayName</display-name-resource>
    <description-resource>searchBrowseDescription</description-resource>
    <destination-page>
      <url>/adminui/searchadmin/browse_all_projects.jsp</url>
      <acl>Profile$role$srchSearchAdminRole:read</acl>
    </destination-page>
  </activity>
  <activity>
    <id>searchWorkbench</id>
    <resource-bundle>atg.searchadmin.adminui.activity.activityresources</resource-bundle>
    <display-name-resource>searchWorkbenchDisplayName</display-name-resource>
    <description-resource>searchWorkbenchDescription</description-resource>
    <destination-page>
      <url>/adminui/workbench/search_workbench.jsp?activeProjectSelection=hide</url>
      <acl>Profile$role$srchSearchAdminRole:read</acl>
    </destination-page>
  </activity>
</generic-activities>
