    <body-content>JSP</body-content>
    <description>The AssetRegistryTree tag encapsulates the creation of an iFrame or Frame object that in-turn calls the registryTree JSP file to allow a hierarchical asset registry tree to be created recursively</description>

    <attribute>
      <name>id</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The id used to identify the asset registry tree container</description>
    </attribute>

    <attribute>
      <name>width</name>
