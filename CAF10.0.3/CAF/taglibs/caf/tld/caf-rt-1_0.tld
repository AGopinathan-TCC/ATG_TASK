      <description>The width of the asset registry tree container in pixels or percentages</description>
    </attribute>

    <attribute>
      <name>height</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The height of the asset registry tree container in pixels or percentages</description>
    </attribute>

    <attribute>
      <name>css</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The CSS of the component</description>
    </attribute>

    <attribute>
      <name>containerType</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>A String that specifies the container type. The default is an iFrame, but the tag is able to support a Frame too</description>
    </attribute>

    <attribute>
      <name>transparent</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>A boolean value that determines whether the iFrame component is rendered transparent</description>
    </attribute>

    <attribute>
      <name>frameBorder</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>A boolean value that determines whether the iFrame component is rendered with a surrounding border</description>
    </attribute>

    <attribute>
      <name>scrolling</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>A boolean value that determines whether scroll bars may be added to the iFrame</description>
    </attribute>

    <attribute>
      <name>src</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The name and path of the JSP file that will contain the tree recursion code</description>
    </attribute>

    <attribute>
      <name>registryName</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The name of the registry to be displayed</description>
    </attribute>

    <attribute>
      <name>registryManagerPath</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
