<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<!DOCTYPE taglib
        PUBLIC "-//Art Technology Group, Inc.//DTD General SQL Adapter//EN"
        "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">

<gsa-template>

  <item-descriptor name="gift-list">
    <table name="dcs_giftlist" type="primary">
      <property name="eventType">
        <attribute name="resourceBundle" value="atg.projects.store.gifts.GiftListsTemplateResources"/>
        <option code="8" xml-combine="remove"/>
        <option code="9" xml-combine="remove"/>
        <option code="101" resource="housewarming"/>
      </property>
    </table>
  </item-descriptor>

  <!--///////////////////////////////-->
  <!--         gift-item             -->
  <!--///////////////////////////////-->

  <item-descriptor name="gift-item">
    <property name="quantityRemaining" data-type="long" queryable="false" writable="false" category="Gift" property-type="atg.projects.store.gifts.GiftlistQuantityRemaining"/>
  </item-descriptor>

</gsa-template>
