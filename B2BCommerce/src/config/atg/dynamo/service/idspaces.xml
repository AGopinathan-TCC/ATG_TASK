<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<id-spaces>
  <!-- 
  Add id spaces for invoice processing
    invoiceRequest is used for the invoice request payment method
    invoice is used for InvoiceRepository ids
    deliveryInfo is used for InvoiceRepository ids
    paymentTerms is used for InvoiceRepository ids
    invoiceNumber is used for user-visible invoice numbers 
  -->

  <id-space name="costCenter" seed="1" batch-size="10000" prefix="cctr"/>
  <id-space name="scheduledOrder" seed="1" batch-size="10000" prefix="sco"/>
  <id-space name="invoice" seed="1" batch-size="1000" prefix="invc"/>
  <id-space name="deliveryInfo" seed="1" batch-size="1000" prefix="di"/>
  <id-space name="paymentTerms" seed="1" batch-size="1000" prefix="pt"/>
  <id-space name="invoiceNumber" seed="1" batch-size="1000" prefix=""/>
  <id-space name="cost-center" seed="1" batch-size="10000" prefix="ctcr"/>
</id-spaces>
