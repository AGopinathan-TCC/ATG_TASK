<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<!DOCTYPE portal-manifest PUBLIC "-//Art Technology Group, Inc.//DTD Portal Manifest//EN" "http://www.atg.com/dtds/portal/framework/portal_manifest_1.0.dtd">

<portal-manifest name="Default Color Palettes" version="1.0" author="ATG Portal Team">

  <color-palettes>

    <color-palette name="Blue Metrix" version="1.0" author="ATG Portal Team">
      <description>Blue Metrix</description>
      <servlet-context>/portal/layoutTemplates</servlet-context>
      <images>
        <image type="small">
          <url>/images/color_theme_blue_metrix.gif</url>
        </image>
      </images>
      <entity type="page-body">
        <colors>
          <color type="background">99ccff</color>
          <color type="text">000000</color>
          <color type="link">0033ff</color>
          <color type="action-link">990000</color>
          <color type="visited-link">336699</color>
        </colors>
      </entity>      
      <entity type="page-body">
        <images>
          <image type="background">
            <url>/images/bg_colorscheme_bluemetrix.gif</url>
          </image>
        </images>
      </entity>      
      <entity type="gear-body">
        <colors>
          <color type="background">ffffff</color>
          <color type="text">000000</color>
        </colors>
      </entity>
      <entity type="gear-title">
        <colors>
          <color type="background">4a8cd6</color>
          <color type="text">ffffff</color>
        </colors>
      </entity>      
    </color-palette>

    <color-palette name="Chrome" version="1.0" author="ATG Portal Team">
      <description>The New Black</description>
      <servlet-context>/portal/layoutTemplates</servlet-context>
      <images>
        <image type="small">
          <url>/images/color_theme_chrome.gif</url>
        </image>
      </images>
      <entity type="page-body">
        <colors>
          <color type="background">999999</color>
          <color type="text">333333</color>
          <color type="link">6666ff</color>
          <color type="action-link">996633</color>
          <color type="visited-link">336699</color>
        </colors>
      </entity>      
      <entity type="page-body">
        <images>
          <image type="background">
            <url>/images/bg_colorscheme_bluemetrix.gif</url>
          </image>
        </images>
      </entity>      
      <entity type="gear-body">
        <colors>
          <color type="background">cccccc</color>
          <color type="text">333333</color>
        </colors>
      </entity>
      <entity type="gear-title">
        <colors>
          <color type="background">333333</color>
          <color type="text">cccccc</color>
        </colors>
      </entity>      
    </color-palette>

  </color-palettes>

</portal-manifest>
