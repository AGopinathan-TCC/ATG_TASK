<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<dataset>
  <registry-descriptor>
    <display-name>Gear Favorite Community Add</display-name>
    <description>Store information when a user adds a community to their Favorite Communities list in a portal application</description>
    <timekey>true</timekey>
    <mapper>/FavoriteAddedSqlLogger.xml</mapper>
    <id>/gearfavoritecommu.xml</id>
  </registry-descriptor>
  <sampling>
    <filter>all</filter>
    <units>percent</units>
    <rate>1</rate>
  </sampling>
</dataset>
