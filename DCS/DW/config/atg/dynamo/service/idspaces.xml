<!-- ===============================================================
=== $Id: //product/DCS/version/10.0.3/DW/config/atg/dynamo/service/idspaces.xml#2 $$Change: 651448 $
=== Default initial id spaces for logging id generators. It is best not to
=== edit this file. Instead create your own XML file of the same
=== name in your localconfig directory and let Dynamo's XML
=== combination facility combine the files.
================================================================ -->

<!DOCTYPE idgenerator
  PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo Id Generator//EN"
  'http://www.atg.com/dtds/idgen/idgenerator_1.0.dtd'>

<id-spaces>

  <id-space name="arf_category"           seed="1" batch-size="100"/>
  <id-space name="arf_product"            seed="1" batch-size="100"/>
  <id-space name="arf_sku"                seed="1" batch-size="100"/>
  <id-space name="arf_promotion"          seed="1" batch-size="10"/>
  <id-space name="arf_promogrp"           seed="1" batch-size="100"/>
  <id-space name="arf_line_item"          seed="1" batch-size="1000"/>
  <id-space name="arf_order"              seed="1" batch-size="1000"/>

</id-spaces>
