<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<dataset>
  <registry-descriptor>
    <display-name>Gear Document View</display-name>
    <description>Store information when a user views or downloads a document from a Document Exchange gear in a portal application</description>
    <timekey>true</timekey>
    <mapper>/DocumentViewedSqlLogger.xml</mapper>
    <id>/geardocumentview.xml</id>
  </registry-descriptor>
  <sampling>
    <filter>all</filter>
    <units>percent</units>
    <rate>1</rate>
  </sampling>
</dataset>
