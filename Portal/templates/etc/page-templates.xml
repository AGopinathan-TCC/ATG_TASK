<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<!DOCTYPE portal-manifest PUBLIC "-//Art Technology Group, Inc.//DTD Portal Manifest//EN" "http://www.atg.com/dtds/portal/framework/portal_manifest_1.0.dtd">

<portal-manifest name="Default Page Templates" version="1.0" author="ATG Portal Team">

  <page-templates>
 
    <page-template name="Default Page Template" version="1.0" author="ATG Portal Team">
      <description>Default Page Template</description>
      <servlet-context>/portal/layoutTemplates</servlet-context>

      <display-modes>
        <display-mode name="shared">
          <device-output name="html">/html/shared_page_template.jsp</device-output>
          <device-output name="wml">/wml/shared_deck_template.jsp</device-output>
          <device-output name="chtml">/chtml/shared_page_template.jsp</device-output>
        </display-mode>
        <display-mode name="full">
          <device-output name="html">/html/full_page_template.jsp</device-output>
          <device-output name="wml">/wml/full_deck_template.jsp</device-output>
          <device-output name="chtml">/chtml/full_page_template.jsp</device-output>
        </display-mode>
      </display-modes>
    </page-template>

  </page-templates>

</portal-manifest>
