<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE portal-manifest PUBLIC "-//Art Technology Group, Inc.//DTD Portal Manifest//EN" "http://www.atg.com/dtds/portal/framework/portal_manifest_1.0.dtd">
<portal-manifest author="ATG Portlet Deploy Tool" name="WSRP4ATGProxyPortlet" version="1.0" >
<gear-definitions>
<gear-definition name="WSRP4ATGProxyPortlet" version="1.0" author="ATG Portlet Deploy Tool">
<description>A Generic proxy portlet which demonstrates WSRP integration with ATG Portal.</description>
<servlet-context>/portlet/WSRPProxyPortlet</servlet-context>
<dimensions>
<dimension name="height">noPreference</dimension>
<dimension name="width">narrow</dimension>
</dimensions>
<gear-modes>
<gear-mode name="content">
<display-modes>
<display-mode name="full">
<device-output name="html">/WSRP4ATGProxyPortlet</device-output>
</display-mode>
<display-mode name="shared">
<device-output name="html">/WSRP4ATGProxyPortlet</device-output>
</display-mode>
</display-modes>
</gear-mode>
<gear-mode name="installConfig">
<display-modes>
<display-mode name="full">
<device-output name="html">/InstallConfig.jsp</device-output>
</display-mode>
</display-modes>
</gear-mode>
<gear-mode name="instanceConfig">
<display-modes>
<display-mode name="full">
<device-output name="html">/InstanceConfig.jsp</device-output>
</display-mode>
</display-modes>
</gear-mode>
<gear-mode name="preview">
<display-modes>
<display-mode name="full">
<device-output name="html">/WSRP4ATGProxyPortlet</device-output>
</display-mode>
<display-mode name="shared">
<device-output name="html">/WSRP4ATGProxyPortlet</device-output>
</display-mode>
</display-modes>
</gear-mode>
</gear-modes>
<jps-portlet value="true">
<portlet-guid value="proxy-portlet.war.WSRP4ATGProxyPortlet"></portlet-guid>
</jps-portlet>
<instance-parameters>
<parameter name="atg.portlet-class">
<default-value>atg.wsrp.consumer.ProxyPortlet</default-value>
</parameter>
<parameter name="titleBarPagePath">
<default-value>/inner_title_bar.jsp</default-value>
</parameter>
<parameter name="notConfiguredPagePath">
<default-value>/index.jsp</default-value>
</parameter>
<parameter name="errorPagePath">
<default-value>/error.jsp</default-value>
</parameter>  
<parameter name="producerID">
<default-value></default-value>
</parameter>
<parameter name="portletDescriptionID">
<default-value></default-value>
</parameter>
</instance-parameters>

</gear-definition>
</gear-definitions>
</portal-manifest>
