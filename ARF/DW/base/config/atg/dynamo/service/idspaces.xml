  <id-space name="arf_stimulus"                 seed="1" batch-size="10"/>
  <id-space name="arf_stimgrp"                  seed="1" batch-size="10"/>
  <id-space name="arf_user"                     seed="1" batch-size="100"/>
  <id-space name="arf_organization"             seed="1" batch-size="100"/>
  <id-space name="arf_site_visit"               seed="1" batch-size="1000"/>
  <id-space name="arf_question_type"            seed="1" batch-size="10"/>
  <id-space name="arf_question_type_group"      seed="1" batch-size="100"/>
  <id-space name="arf_search_number"            seed="1" batch-size="100"/>
  <id-space name="arf_search_number_group"      seed="1" batch-size="100"/>
  <id-space name="arf_question"                 seed="1" batch-size="100"/>
  <id-space name="arf_site"                     seed="1" batch-size="100"/>
  <id-space name="arf_user_agent"               seed="1" batch-size="10"/>
  <id-space name="arf_referrer"                 seed="1" batch-size="10"/>
  <id-space name="arf_registration"             seed="1" batch-size="1000"/>


</id-spaces>
