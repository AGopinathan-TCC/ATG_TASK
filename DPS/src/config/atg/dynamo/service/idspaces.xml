<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<!-- ===============================================================
=== $Id: //product/DPS/version/10.0.3/templates/DPS/config/userprofiling/atg/dynamo/service/idspaces.xml#2 $$Change: 651448 $
=== Default initial id spaces for logging id generators. It is best not to
=== edit this file. Instead create your own XML file of the same
=== name in your localconfig directory and let Dynamo's XML
=== combination facility combine the files.
================================================================ -->

<!DOCTYPE idgenerator
  PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo Id Generator//EN"
  'http://www.atg.com/dtds/idgen/idgenerator_1.0.dtd'>

<id-spaces>

  <!-- id space used by UserEventLogging -->
  <id-space name="dps_user_event" seed="0" batch-size="10000"/>

  <!-- id space used by ContentViewedLogging -->
  <id-space name="dps_content_request" seed="0" batch-size="10000"/>

  <!-- id space used by  RequestLogging -->
  <id-space name="dps_request" seed="0" batch-size="10000"/>


</id-spaces>
