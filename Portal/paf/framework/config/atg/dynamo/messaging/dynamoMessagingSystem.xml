	      </description-resource>
	      <resource-bundle>
	        atg.portal.messaging.GearMessagingSystemResources
	      </resource-bundle>
      </message-type>

      <message-type>
        <jms-type>
	        atg.portal.messaging.CommunityGearRemovedMessage
        </jms-type>
	      <message-class>
