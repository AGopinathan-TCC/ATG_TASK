<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE portal-manifest PUBLIC "-//Art Technology Group, Inc.//DTD Portal Manifest//EN" "http://www.atg.com/dtds/portal/framework/portal_manifest_1.0.dtd">

<portal-manifest name="Servlet Dump" version="1.0" author="ATG Portal Team">

  <gear-definitions>
    <gear-definition name="Servlet Dump" version="1.0" author="ATG Portal Team">
      <description>This gear displays the contents of a servlet request. It is intended for testing purposes.</description>

      <servlet-context>/portal</servlet-context>

      <dimensions>
        <dimension name="height">noPreference</dimension>
        <dimension name="width">noPreference</dimension>
      </dimensions>

      <gear-modes>

        <gear-mode name="content">
          <display-modes>
            <display-mode name="shared">
              <device-output name="html">/portlets/servlet-dump/content/html/content.jsp</device-output>
              <device-output name="wml">/portlets/servlet-dump/content/wml/content.jsp</device-output>
            </display-mode>
            <display-mode name="full">
              <device-output name="html">/portlets/servlet-dump/content/html/content.jsp</device-output>
              <device-output name="wml">/portlets/servlet-dump/content/wml/content.jsp</device-output>
            </display-mode>
          </display-modes>
        </gear-mode>

        <gear-mode name="about">
          <display-modes>
            <display-mode name="shared">
              <device-output name="html">/portlets/servlet-dump/about/html/about.jsp</device-output>
              <device-output name="wml">/portlets/servlet-dump/about/wml/about.jsp</device-output>
            </display-mode>
            <display-mode name="full">
              <device-output name="html">/portlets/servlet-dump/about/html/about.jsp</device-output>
              <device-output name="wml">/portlets/servlet-dump/about/wml/about.jsp</device-output>
            </display-mode>
          </display-modes>
        </gear-mode>

        <gear-mode name="help">
          <display-modes>
            <display-mode name="shared">
              <device-output name="html">/portlets/servlet-dump/help/html/help.jsp</device-output>
              <device-output name="wml">/portlets/servlet-dump/help/wml/help.jsp</device-output>
            </display-mode>
            <display-mode name="full">
              <device-output name="html">/portlets/servlet-dump/help/html/help.jsp</device-output>
              <device-output name="wml">/portlets/servlet-dump/help/wml/help.jsp</device-output>
            </display-mode>
          </display-modes>
        </gear-mode>

      </gear-modes>
    </gear-definition>

  </gear-definitions>
</portal-manifest>
