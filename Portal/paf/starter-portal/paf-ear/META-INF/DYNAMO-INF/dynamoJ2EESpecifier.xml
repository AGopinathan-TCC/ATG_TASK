<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE dynamo-j2ee-specifier PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo J2EE Application Specifier 1.3//EN" "http://www.atg.com/j2ee/dtds/dynamoJ2EESpecifier_1_3.dtd">
<dynamo-j2ee-specifier>

  <application-name>Portal Application Framework</application-name>

  <user-manager>
    dynamo:/atg/userprofiling/ProfileUserAuthority
  </user-manager>

  <war>
     <module-uri>admin.war</module-uri>
    <web-application-mapping>
        <uri-prefix>/portal/admin</uri-prefix>
    </web-application-mapping> 
   </war>

  <war>
     <module-uri>settings.war</module-uri>
    <web-application-mapping>
        <uri-prefix>/portal/settings</uri-prefix>
    </web-application-mapping> 
  </war>

  <war>
      <module-uri>portal.war</module-uri>
    <web-application-mapping>
        <uri-prefix>/portal</uri-prefix>
    </web-application-mapping> 
  </war>

</dynamo-j2ee-specifier>
